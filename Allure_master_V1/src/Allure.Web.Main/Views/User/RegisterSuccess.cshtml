<link type="text/less" rel="stylesheet" href="/static/css/page/registerSuccess.css">
<!-- register start-->
	<div class="al-register-success-wrap">
		<div class="al-register-success-area">
			<div class="al-register-success-info">
                <div class="al-register-success-text">
                    <h3>Activate Account</h3>
                    <div>
                        <div class="alert alert-success" role="alert">
                            <span>Dear user, please click this link to activate your account.</span>
                            <a href="@ViewBag.ActivateUrl">@ViewBag.ActivateUrl</a>
                        </div>
                    </div>
                </div>
			</div>
		</div>
	</div>
<!-- register end-->

<script type="text/javascript">
    var require = (function () {

        return {
            "callback": function () {

                var deps = ["require", "jquery"];

                require.config({
                    "waitSeconds": 0,

                    "baseUrl": "/static",

                    "packages": [
                        {
                            "name": "jquery",
                            "location": "js/lib/jquery",
                            "main": "jquery-1.9.1"
                        },
                        {
                            "name": "bootstrap",
                            "location": "js/plugin/bootstrap",
                            "main": "bootstrap"
                        }
                    ],

                    "shim": {
                        "bootstrap": {
                            deps: ["jquery"],
                            init: function($) {
                                return $;
                            }
                        }
                    },

                    "deps": deps,

                    "callback": function (localRequire) {
                        localRequire(["jquery"], function (jQuery) {
                            jQuery(document.body).ready(
                                function(){
                                    //new alHome();
                            });
                        });
                    }
                });
            }
        };
    })();
</script>