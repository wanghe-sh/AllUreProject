<!-- FURNITURE display start-->
<div class="al-display-wrap">
    <div class="al-display-area">
        <div class="al-display-info">
            <!-- one row start-->
            <div class="box-list two">
                <div class="box-list-title">
                    <h4>FURNITURE DISPLAY BIG PICUTRE</h4>
                </div>

                <!-- ELECTRIC APPLIANCE slide start-->
                <div class="al-slide-wrap">
                    <div class="al-slide-area">
                        <div class="al-slide-info">
                            <div id="carousel-furniture-appliance" class="al-carousel carousel slide">
                                <ol class="carousel-indicators">
                                    <li data-target="#carousel-furniture-appliance" data-slide-to="0" class="active"></li>
                                    <li data-target="#carousel-furniture-appliance" data-slide-to="1"></li>
                                    <li data-target="#carousel-furniture-appliance" data-slide-to="2"></li>
                                </ol>
                                <div class="carousel-inner" role="listbox">
                                    @for (int i = 0; i < recommendL.Length; i++)
                                    {
                                        <div class="item @(i == 0 ? "active" : string.Empty)">
                                            <img src="@(this.AdminSite + recommendL[i].Images.First(m => m.IsLargeImage).ImageUrl)" data-holder-rendered="true">
                                            <div class="carousel-caption">
                                                <h3>@recommendL[i].Name</h3>
                                                <p>@recommendL[i].Localized.Descrption</p>
                                            </div>
                                        </div>
                                    }
                                </div>
                                <a class="left carousel-control" href="#carousel-furniture-appliance" role="button" data-slide="prev">
                                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a class="right carousel-control" href="#carousel-furniture-appliance" role="button" data-slide="next">
                                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ELECTRIC APPLIANCE slide end-->
                <div class="box-des">
                    Descript text: this is the text which show furniture description.
                </div>
                <div class="list">
                    @for (int i = 0; i < recommendS.Length; i++)
                    {
                        <div class="box">
                            <a href="/product/@recommendS[i].Id" class="rel">
                                <div class="abs">
                                    <div class="con">
                                        <h3>@recommendS[i].Name</h3>
                                        <h5>@recommendS[i].Localized.Descrption</h5>
                                    </div>
                                </div>
                                <img class="media-object" src="@(AdminSite + recommendS[i].Images.First(m => !m.IsLargeImage).ImageUrl)" />
                            </a>
                        </div>
                    }
                </div>
            </div>
            <!-- one row end-->

        </div>
    </div>
</div>
<div class="al-line-between"></div>
<!-- FURNITURE display end-->



