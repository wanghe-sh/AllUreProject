                <!-- ELECTRIC APPLIANCE slide start-->
                <div class="al-slide-wrap">
                    <div class="al-slide-area">
                        <div class="al-slide-info">
                            <div id="carousel-furniture-appliance" class="al-carousel carousel slide">
                                <ol class="carousel-indicators">
                                    @for (int i = 0; i < recommendL.Length && i < 5; i++)
                                    {
                                        if (i == 0)
                                        {
                                            <li data-target="#carousel-furniture-appliance" data-slide-to="@i" class="active"></li>
                                        }
                                        else
                                        {
                                            <li data-target="#carousel-furniture-appliance" data-slide-to="@i"></li>
                                        }
                                    }
                                </ol>
                                <div class="carousel-inner" role="listbox">
                                    @for (int i = 0; i < recommendL.Length && i < 5; i++)
                                    {

                                        <div class="item @(i == 0 ? "active" : string.Empty)">
                                            <a href="/product/@recommendL[i].Id" class="rel">
                                                <img src="@(recommendL[i].Images.FirstOrDefault(m => m.Height >= 400 && m.Width >= 940) == null ? "" : recommendL[i].Images.FirstOrDefault(m => m.Height >= 400 && m.Width >= 940).Url)" data-holder-rendered="true">
                                            </a>
                                                <div class="carousel-caption">
                                                    <h3>@recommendL[i].Name</h3>
                                                    @*<p>@recommendL[i].Localized.Descrption</p>*@
                                                </div>
</div>
                                        
                                    }
</div>
                                <a class="left carousel-control" href="#carousel-furniture-appliance" role="button" data-slide="prev">
                                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a class="right carousel-control" href="#carousel-furniture-appliance" role="button" data-slide="next">
                                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                
                </div>
            <!-- one row end-->
        </div>
    </div>
</div>

