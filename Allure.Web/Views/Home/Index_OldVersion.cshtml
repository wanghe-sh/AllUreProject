                    "deps": deps,

                    "callback": function (localRequire) {
                        localRequire(["jquery", "alHome"], function (jQuery, alHome) {
                            jQuery(document.body).ready(
                                function () {
                                    new alHome();
                                });
                        });
                    }
                });
            }
        };
    })();
</script>
